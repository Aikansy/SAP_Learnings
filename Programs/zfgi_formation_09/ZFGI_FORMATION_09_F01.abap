FORM create_object.

    CREATE OBJECT go_alv_travel
      EXPORTING
        iv_driver_id = p_driver
        iv_cityf = p_cityf
        iv_cityt = p_cityt
        iv_date_travel = p_datet.
    
      CALL METHOD go_alv_travel->get_travel.
    
    ENDFORM.
    
